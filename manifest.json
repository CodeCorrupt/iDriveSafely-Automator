{
  "manifest_version": 2,

  "name": "iDriveSafely - Cheat",
  "description": "This extension will automatically click next.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.idrivesafely.com/*"],
      "js": ["iDriveSafely.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
